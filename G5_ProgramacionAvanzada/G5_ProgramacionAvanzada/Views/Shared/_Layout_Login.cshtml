<!DOCTYPE html>
<html lang="es">


<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Home - Andtourtravel </title>
    <link rel="stylesheet" href="~/Templates/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Templates/css/animate.min.css" />
    <link rel="stylesheet" href="~/Templates/css/fontawesome.all.min.css" />
    <link rel="stylesheet" href="../../../../../cdn.jsdelivr.net/npm/bootstrap-icons%401.8.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/Templates/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/Templates/css/owl.theme.default.min.css" />
    <link rel="stylesheet" href="~/Templates/css/navber.css" />
    <link rel="stylesheet" href="~/Templates/css/meanmenu.css" />
    <link rel="stylesheet" href="~/Templates/css/style.css" />
    <link rel="stylesheet" href="~/Templates/css/responsive.css" />
    <link rel="icon" type="image/png" href="~/Templates/img/favicon.png">
</head>

<body>
    <!-- Header Area -->
    <header class="main_header_arae">
        <div class="topbar-area">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-6 col-md-6">
                        <ul class="topbar-list">
                            <li>
                                <a href="#!"><i class="fab fa-facebook"></i></a>
                                <a href="#!"><i class="fab fa-twitter-square"></i></a>
                                <a href="#!"><i class="fab fa-instagram"></i></a>
                                <a href="#!"><i class="fab fa-linkedin"></i></a>
                            </li>
                            <li><a href="#!"><span>+011 234 567 89</span></a></li>
                            <li><a href="#!"><span>contact@domain.com</span></a></li>
                        </ul>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <ul class="topbar-others-options">
                            <li>
                                <a href="@Url.Action("IniciarSesion","Login")">
                                    <i class="ti-home text-primary"></i>
                                    Iniciar Sesión
                                </a>
                            </li>
                            <li>
                                <a href=">
                                    <i class="ti-home text-primary"></i>
                                    Registrarse
                                </a>
                            </li>
                            <li>
                                <div class="dropdown language-option">
                                    <button class="dropdown-toggle" type="button" data-bs-toggle="dropdown"
                                            aria-haspopup="true" aria-expanded="false">
                                        <span class="lang-name"></span>
                                    </button>
                                    <div class="dropdown-menu language-dropdown-menu">
                                        <a class="dropdown-item" href="#">English</a>
                                        <a class="dropdown-item" href="#">Arabic</a>
                                        <a class="dropdown-item" href="#">French</a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        @*PANEL PRINCIPAL*@
        <div class="main-panel" style="width: 100%;">
            <div class="content-wrapper">
                <div class="row">
                    <div class="col-md-12 grid-margin">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!-- Footer  -->
            <footer id="footer_area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                            <div class="footer_heading_area">
                                <h5>¿Necesitas ayuda?</h5>
                            </div>
                            <div class="footer_first_area">
                                <div class="footer_inquery_area">
                                    <h5>Llama a nuestro call center por ayuda</h5>
                                    <h3> <a href="tel:+00-123-456-789">+00 123 456 789</a></h3>
                                </div>
                                <div class="footer_inquery_area">
                                    <h5>Síguenos en</h5>
                                    <ul class="soical_icon_footer">
                                        <li><a href="#!"><i class="fab fa-facebook"></i></a></li>
                                        <li><a href="#!"><i class="fab fa-twitter-square"></i></a></li>
                                        <li><a href="#!"><i class="fab fa-instagram"></i></a></li>
                                        <li><a href="#!"><i class="fab fa-linkedin"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
        </div>
        </footer>
        <div class="copyright_area">
            <div class="container">
                <div class="row align-items-center">
                    <div class="co-lg-6 col-md-6 col-sm-12 col-12">
                        <div class="copyright_left">
                            <p>Copyright © 2022 All Rights Reserved</p>
                        </div>
                    </div>
                    <div class="co-lg-6 col-md-6 col-sm-12 col-12">
                        <div class="copyright_right">
                            <img src="~/Templates/img/common/cards.png" alt="img">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="go-top">
            <i class="fas fa-chevron-up"></i>
            <i class="fas fa-chevron-up"></i>
        </div>

        <script src="~/Templates/js/jquery-3.6.0.min.js"></script>
        <script src="~/Templates/js/bootstrap.bundle.js"></script>
        <script src="~/Templates/js/jquery.meanmenu.js"></script>
        <script src="~/Templates/js/owl.carousel.min.js"></script>
        <script src="~/Templates/js/wow.min.js"></script>
        <script src="~/Templates/js/custom.js"></script>
        <script src="~/Templates/js/add-form.js"></script>
        <script src="~/Templates/js/form-dropdown.js"></script>
</body>

</html>
